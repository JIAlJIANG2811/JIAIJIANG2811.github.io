<!--找个放地图的框~~-->
<div id="map">
    <div class="form-button-container">
        <div id="form-container">
            <form id="address-form">
                <input type="text" id="address" name="address" placeholder="输入地址" required>
                <button id='addressSearch-btn' type="submit">
                    <i class="fa fa-search"></i> <!-- Font Awesome 搜索图标 -->
                </button>
            </form>
        </div>
    </div>
    <div class="buttons">
        <button id="todayWeather-btn" class="icon-btn" title="实时天气">实时天气</button>
        <button id="futureWeather-btn" class="icon-btn" title="预测天气">预测天气</i></button>
        <button id="drawButton" class="icon-btn" title="开始测距">开始测距</i></button>
        <button id="stopButton" class="icon-btn" title="结束测距">结束测距</i></button>
        <button id="removeButton" class="icon-btn" title="清除覆盖物">清除绘制</i></button>
    </div>
    <div id="weather-info-container"></div>
    <!--给图表找一个安神的家（div），给家取名字，定大小~~-->
    <div id="future-weather-chart-container" style="width: 500px;height: 300px"></div>
